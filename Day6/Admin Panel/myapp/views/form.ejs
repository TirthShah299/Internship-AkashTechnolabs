									<div class="col-sm-8">
										<input type="text" class="form-control1" id="focusedinput" placeholder="Name of the item">
									</div>
								</div>
								<div class="form-group">
									<label for="disabledinput" class="col-sm-2 control-label">Price: </label>
									<div class="col-sm-8">
										<input disabled="" type="text" class="form-control1" id="disabledinput" placeholder="Enter Price">
									</div>
								</div>
								
							</form>
						</div>
			</div>
					
					<div class="bs-example" data-example-id="form-validation-states">
        </div>
  
